{
  "name": "brightalley/laravel-lti-1p3",
  "description": "A Laravel wrapper for https://github.com/celtic-project/LTI-PHP",
  "keywords": ["laravel","lti"],
  "license": "LGPL-3.0-or-later",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/brightalley/LTI-1p3-Tool-Provider-Library-PHP.git"
    }
  ],
  "require": {
    "ext-pdo": "*",
    "brightalley/lti-1p3": "dev-master",
  },
  "autoload": {
    "psr-4": {
      "LonghornOpen\\LaravelCelticLTI\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LonghornOpen\\LaravelCelticLTI\\LtiServiceProvider"
      ],
      "aliases": {
        "LTI": "LonghornOpen\\LaravelCelticLTI\\LtiFacade"
      }
    }
  }
}
