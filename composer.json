{
  "name": "brightalley/laravel-lti-1p3",
  "description": "A Laravel wrapper for https://github.com/longhornopen/LTI-PHP",
  "keywords": ["laravel","lti"],
  "license": "LGPL-3.0-or-later",
  "require": {
    "longhornopen/lti": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "LonghornOpen\\LaravelCelticLTI\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LonghornOpen\\LaravelCelticLTI\\LtiServiceProvider"
      ],
      "aliases": {
        "LTI": "LonghornOpen\\LaravelCelticLTI\\LtiFacade"
      }
    }
  }
}
